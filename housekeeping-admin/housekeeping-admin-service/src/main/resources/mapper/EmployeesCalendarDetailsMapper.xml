<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.housekeeping.admin.mapper.EmployeesCalendarDetailsMapper">

	<select id="groupByCalendarIdHaving" parameterType="java.util.List" resultType="com.housekeeping.admin.entity.EmployeesCalendarDetails">
		SELECT *
		FROM employees_calendar_details
		GROUP BY calendar_id
		having calendar_id in
			<foreach collection="list" index="index" item="item" open="(" separator="," close=")">
				#{item}
			</foreach>
	</select>
</mapper>
