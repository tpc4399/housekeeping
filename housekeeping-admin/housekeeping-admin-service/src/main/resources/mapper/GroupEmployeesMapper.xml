<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.housekeeping.admin.mapper.GroupEmployeesMapper">

    <select id="count" resultType="java.lang.Integer">
        SELECT COUNT(id) FROM group_employees WHERE group_id = #{groupId}
    </select>

    <select id="getIdsByGroupId" resultType="java.lang.Integer">
        SELECT employees_id FROM group_employees  WHERE group_id  = #{groupId}
    </select>
</mapper>
